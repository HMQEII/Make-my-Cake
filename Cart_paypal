  /*distinguishing table headers from data cells*/
  font-weight: bold;
  text-align: center;
}
input {
  margin-bottom: 10px;
}
caption {
  /*coordinated margin to match cell's padding*/
  margin-bottom: .5em;
  /*centered so it doesn't blend in to other content*/
  text-align: center;
}
