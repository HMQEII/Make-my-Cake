body {
  font-family: "Helvetica Neue", "HelveticaNeue", "HelveticaNeue-Light", "Helvetica Neue Light", helvetica, arial, sans-serif;
  font-size: 95%;
  /* Base font-size is 15px */
  color: #2c2e2f;
  line-height: 1.35em;
}
